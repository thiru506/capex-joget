<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="UserPasswordHistory" name="org.joget.plugin.directory.model.UserPasswordHistory" table="dir_user_password_history">
    <id column="id" name="id" type="string"/>
    <property column="username" name="username" type="string"/>
    <property column="salt" name="salt" type="string"/>
    <property column="password" name="password" type="string"/>
    <property column="updatedDate" name="updatedDate" type="timestamp"/>
  </class>
</hibernate-mapping>
